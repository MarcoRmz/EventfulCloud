<h1><%= title %></h1>

<div id="image_list">

<% if (images.length == 0) { %>
  No Events.
<% } else { %>
   <% _(images).each(function(image) { %>
     <div>
       <a href="/i/<%= image.id %>"><img src="<%= image.get('sizeSmall').url() %>" /></a>
       <p style="text-align: center;"><%= image.get('title') %></p>
     </div>
   <% }); %>
<% } %>
</div>
